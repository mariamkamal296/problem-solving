#include <iostream>
#include <bits/stdc++.h>
 
using namespace std;
 
int main()
{
    int Hmany, Htall, WtoP=0;
    cin>> Hmany ;
    cin>> Htall;
    int* arr = new int[Hmany];
    for(int i =0; i < Hmany; i++)
    {
        cin>>arr[i];
    }
    for(int i =0; i < Hmany; i++)
    {
       if(arr[i]> Htall)      WtoP += 2;   
       else                   WtoP += 1;  
         
    }
    cout << WtoP ;
}
